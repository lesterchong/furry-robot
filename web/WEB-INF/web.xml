<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>AddAdmittanceServlet</servlet-name>
        <servlet-class>SERVLET.AddAdmittanceServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserAuthenticationServlet</servlet-name>
        <servlet-class>SERVLET.UserAuthenticationServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddSupplyServlet</servlet-name>
        <servlet-class>SERVLET.AddSupplyServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserLogoutServlet</servlet-name>
        <servlet-class>SERVLET.UserLogoutServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddDischargeServlet</servlet-name>
        <servlet-class>SERVLET.AddDischargeServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SendTextServlet</servlet-name>
        <servlet-class>SERVLET.SendTextServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AddAdmittanceServlet</servlet-name>
        <url-pattern>/AddAdmittanceServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserAuthenticationServlet</servlet-name>
        <url-pattern>/UserAuthenticationServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddSupplyServlet</servlet-name>
        <url-pattern>/AddSupplyServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserLogoutServlet</servlet-name>
        <url-pattern>/UserLogoutServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddDischargeServlet</servlet-name>
        <url-pattern>/AddDischargeServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SendTextServlet</servlet-name>
        <url-pattern>/SendTextServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
